                     <a class="underline-link" href="/employmentandhousing/hpi"> House Price Index (HPI): Quarterly updated </a>
                  </h3>
                  <p class="flush">
                    HPI tracks changes in residential property prices on the island, providing valuable insights into the housing market's trends and fluctuations.
                 </p>
               </div>
            </div>
            <div class="col col--md-15 col--lg-14">
               <div style="height:96px;">
                  <div class="sparkline" style="height:100%" data-sparkline-ABMI data-uri="/employmentandhousing/hpi">
                     <iframe height="500px" width="100%" frameBorder="0" src="https://stats.je/api/employmentandhousing/hpiSL"></iframe>
                  </div>
               </div>
            </div>
            <div class="col col--md-13 col--lg-18">
               <div class="tiles__content">
   
                  <span class="tiles__preunit tiles__preunit--list" id="percentageDifference" style="margin-right: 10px;"></span>
                  <span class="tiles__unit tiles__unit--list" id="SecondToLastHPIValue" style="display: none;"></span>
                  <span class="tiles__figure tiles__figure--list">(<span id="HPIValue"></span>)</span>

                  <dl class="tiles__dl">
                     <dt class="tiles__dt">Release date:</dt><dd class="flush">Q3 2023</dd>
                     <dt class="tiles__dt">Next release:</dt><dd class="flush">Q4 2023</dd>
                  </dl>
                  <script>
                     window.addEventListener('message', function (event) {
                       if (event.origin === 'https://stats.je') {
                          var hpiValue = event.data.hpiValue;
                          var secondtolasthpiValue = event.data.secondtolasthpiValue;
                     
                          if (hpiValue !== undefined) {
                             var gvaValueSpan = document.getElementById('HPIValue');
                             gvaValueSpan.textContent = hpiValue;
                          }
                     
                          if (secondtolasthpiValue !== undefined) {
                             var secondToLastHPIValueSpan = document.getElementById('SecondToLastHPIValue');
                             secondToLastHPIValueSpan.textContent = secondtolasthpiValue;
                          }
                     
                     const secondToLastHPIValue = parseFloat(document.getElementById("SecondToLastHPIValue").textContent);
                     const HPIValue = parseFloat(document.getElementById("HPIValue").textContent);
                     
                     if (!isNaN(secondToLastHPIValue) && !isNaN(HPIValue)) {
                        const difference = HPIValue - secondToLastHPIValue;
                        const percentageDifference = (difference / secondToLastHPIValue) * 100;
                        const percentageDifferenceElement = document.getElementById("percentageDifference");
                        percentageDifferenceElement.textContent = `${percentageDifference.toFixed(1)}%`;
                     
                     } else {
                        const percentageDifferenceElement = document.getElementById("percentageDifference");
                        percentageDifferenceElement.textContent = "Invalid values";
                        }
                       }
                     }, false);
                  </script>
   
                  </div>
               </div>
