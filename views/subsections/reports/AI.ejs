<script>
  document.addEventListener("DOMContentLoaded", function() {

    const outputElement = document.getElementById('title-output');
    const textArray = [
      "AI & ML ",
      "e/acc",
      "JE | CI | UK"
    ];
    let index = 0;

    function typeText() {
      const currentText = textArray[index];
      const currentTextArray = currentText.split('');
      let newText = '';

      currentTextArray.forEach((char, i) => {
        setTimeout(() => {
          newText += char;
          outputElement.innerText = newText;
        }, i * 100);
      });

      setTimeout(deleteText, currentTextArray.length * 100 + 1000);
    }

    function deleteText() {
      const currentText = textArray[index];
      const currentTextArray = currentText.split('');
      let newText = currentTextArray.join('');

      currentTextArray.forEach((char, i) => {
        setTimeout(() => {
          currentTextArray.pop();
          newText = currentTextArray.join('');
          outputElement.innerText = newText;
        }, i * 50);
      });

      index = (index + 1) % textArray.length;
      setTimeout(typeText, currentTextArray.length * 50 + 1000);
    }

    typeText();

  });
</script>







