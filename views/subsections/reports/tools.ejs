  <script>
    document.addEventListener("DOMContentLoaded", function() {
      
      var textAIJE = "Tools";
      var text = "SearchStat";
      var textAutoReport = "AutoReport";
  
      var typedTextElementAIJE = document.getElementById("typed-text-aije");
      var typedTextElement = document.getElementById("typed-text");
      var typedTextElementAutoReport = document.getElementById("typed-text-auto-report");
  
      function typeText(index, element, text) {
        element.textContent = text.slice(0, index);
        if (index < text.length) {
          setTimeout(function() {
            typeText(index + 1, element, text);
          }, 300);
        }
      }
  
      setTimeout(function() {
        typeText(0, typedTextElementAIJE, textAIJE);
      }, 50);
  
      typeText(0, typedTextElement, text);
  
      setTimeout(function() {
        typeText(0, typedTextElementAutoReport, textAutoReport);
      }, 200);
  
    });
  </script>
  
  