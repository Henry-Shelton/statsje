               }
               
               window.onclick = function(event) {
                 var dropdown = document.getElementById("menu-dropdown");
                 var menuToggle = document.getElementById("menu-toggle");
                 
                 // Check if the clicked element is not inside the dropdown or the menu toggle button
                 if (!event.target.closest('.nav--controls__item') && !event.target.closest('.nav--controls__dropdown')) {
                   dropdown.style.display = "none";
                 }
               };
               
               window.onscroll = function() {
                 var navItem = document.querySelector('.nav--controls__item');
                 var sticky = navItem.offsetTop;
               
                 if (window.pageYOffset > sticky) {
                   navItem.style.position = "fixed";
                   navItem.style.top = "0";
                   navItem.style.width = "100%";
                   navItem.style.zIndex = "1000";
                 } else {
                   navItem.style.position = "relative";
                   navItem.style.top = "auto";
                   navItem.style.width = "100%";
                   navItem.style.zIndex = "auto";
                 }
               };
